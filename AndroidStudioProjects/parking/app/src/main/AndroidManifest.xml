<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_fyp2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Hparking"></activity>
        <activity android:name=".Activity_Gparking" />
        <activity android:name=".Activity_Fparking" />
        <activity android:name=".Activity_Eparking" />
        <activity android:name=".Activity_Dparking" />
        <activity android:name=".Activity_Cparking" />
        <activity android:name=".Activity_Aparking" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>